<form action="{{ path('meetings_new') }}" method="post" {{ form_enctype(form) }}>
    {{ form_errors(form) }}

    {{ form_row(form.date) }}
    {{ form_row(form.time) }}
    {{ form_row(form.duration) }}
    {{ form_row(form.location) }}
    {{ form_rest(form) }}
    <input type="submit" />
</form>

